<!DOCTYPE html>
<html lang="pt-br">
	<head>
		<meta charset="utf-8">
		<title>Inserir orcamentos!</title>
	</head>
	<body>
		<form action="/inserirOrc" method="post">
			<table>
				<tr>
					<td>Cliente</td>
					<td>Equipamento</td>
					<td>Serial Number</td>
					<td>Valor</td>
				</tr>

				<tr>
					<td><input autocomplete="off" type="text" name="cliente" list="cliente"></td>
					<datalist id="cliente">
						<% for (var i=0; i < detalhe.cliente.length; i++) { %>
							<option value="<%= detalhe.cliente[i].nomeCliente %>">
						<% } %>	
					</datalist>
					<td><input autocomplete="off" type="text" name="equipamento" list="equipamento" id="equipamento"></td>
					<datalist id="equipamento">
						<% for (var i=0; i < detalhe.equip.length; i++) { %>
							<option value="<%= detalhe.equip[i].nomeEquip %>">
						<% } %>	
					</datalist>
					<td><input autocomplete="off" type="number" name="serialNumber" id="serialNumber"></td>
					<td><input autocomplete="off" type="number" name="valor" id="valor"></td>
					<td><input type="hidden" name="usuario" id="usuario" value="1"><input type="hidden" name="idUsuario" value="<%= %>"></td>
					<td><input type="hidden" name="id" id="id" value="<%= %>"></td>
					<td><input type="submit" value="Submit"></td>
				</tr>
			
			</table>
		</form>
	<script>
		function eliminateDuplicates(arr) {
			var i,
				len = arr.length,
				out = [],
				obj = {};

			for (i = 0; i < len; i++) {
				obj[arr[i]] = 0;
			}
			for (i in obj) {
				out.push(i);
			}
			return out;
		}
	</script>
	</body>
</html>